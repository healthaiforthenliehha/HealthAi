{
  "expo": {
    "name": "HealthAi",
    "scheme": "healthai",
    "slug": "HealthAi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "package": "com.health.ai",
      "config": {
        "usesHealthKit": true
      },
      "infoPlist": {
        "NSHealthShareUsageDescription": "Diese App benötigt Zugriff auf Ihre Gesundheitsdaten, um Ihre täglichen Schritte anzuzeigen.",
        "UIBackgroundModes": [
          "fetch"
        ]
      }
    },
    "android": {
      "permissions": [
        "BACKGROUND_FETCH"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-health",
        {
          "healthSharePermission": "Allow HealthAi to check health info.",
          "healthUpdatePermission": "Allow HealthAi to update health info.",
          "isClinicalDataEnabled": false,
          "healthClinicalDescription": "Allow HealthAi to check clinical health info."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "190ad2d4-99d3-444c-a2bc-7daa0fd69b62"
      }
    },
    "owner": "devadrian"
  }
}
